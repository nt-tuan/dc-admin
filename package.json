{
  "name": "distichain-admin-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "set PORT=3001 && react-app-rewired start",
    "build": "react-app-rewired build",
    "build-dev": "env-cmd -f .env.dev react-app-rewired build",
    "build-qat": "env-cmd -f .env.qat react-app-rewired build",
    "build-staging": "env-cmd -f .env.staging react-app-rewired build",
    "build-prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"src/**/*.{js,jsx,ts,tsx,scss,less}\"",
    "eslint": "eslint --ext .js,jsx src",
    "stylelint": "stylelint \"src/**/*.{scss,less}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,scss,less}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.{js,jsx}": [
      "eslint"
    ],
    "src/**/*.{scss,less}": [
      "stylelint"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.1",
    "@ant-design/icons": "^4.3.0",
    "@react-pdf/renderer": "^1.6.10",
    "fingerprintjs2": "^2.1.2",
    "@stomp/stompjs": "^5.4.4",
    "antd": "4.8.5",
    "antd-dayjs-webpack-plugin": "^1.0.0",
    "antd-img-crop": "^3.13.2",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "connected-react-router": "^6.8.0",
    "dayjs": "^1.8.27",
    "env-cmd": "^10.0.1",
    "history": "^4.10.1",
    "leaflet": "^1.6.0",
    "leaflet-map": "^0.2.1",
    "lodash": "^4.17.15",
    "nprogress": "^0.2.0",
    "qs": "^6.7.0",
    "numeral": "^2.0.6",
    "react": "^16.13.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet": "^6.0.0",
    "react-highlight-words": "^0.16.0",
    "react-lazyload": "^2.6.7",
    "react-leaflet": "^2.6.3",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.1.2",
    "react-router-transition-switch": "1.1.2",
    "react-scripts": "3.4.1",
    "react-spring": "^8.0.27",
    "react-transition-group": "^4.3.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3",
    "sockjs-client": "^1.5.0",
    "store": "^2.0.12",
    "xlsx": "^0.15.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "babel-plugin-import": "^1.13.0",
    "customize-cra": "^0.9.1",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.10.1",
    "husky": "^4.2.5",
    "less": "^3.11.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.1.3",
    "node-sass": "^4.13.1",
    "prettier": "2.0.4",
    "react-app-rewired": "^2.1.5",
    "stylelint": "^13.3.2",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "worker-loader": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
